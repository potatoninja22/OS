{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/MinGW/include"  // Add the MinGW include directory path here
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "C:/MinGW/bin/gcc.exe",  // Path to GCC compiler in MinGW installation
            "cStandard": "c11",  // Use "c11" for C standard
            "cppStandard": "c++17",  // Use "c++17" for C++ standard
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}
